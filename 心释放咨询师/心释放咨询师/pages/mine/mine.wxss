/* pages/mine/mine.wxss */
page{
    background-color: #f9dc74;
}
.touxiang{
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
}
.touxiangbox{
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
    box-shadow: 0 0 10rpx rgb(131, 130, 130);
    background-color: #fff;
    margin: 0 auto;
}
.top{
    width: 300rpx;
    height:300rpx;
    margin: 10rpx auto;
    text-align: center;
}

.middle{
    width: 90%;
    background-color: #fff;
    border-top-left-radius: 100rpx;
    border-top-right-radius: 100rpx;
    padding: 40rpx;
}

.box{
    width: 200rpx;
    height: 200rpx;
    text-align: center;
    line-height: 80rpx;
}
.ziliao{
    display: inline-block;
}
.ziliao text,.add{
    background-color: rgb(247, 186, 186);
    display: inline-block;
    height: 40rpx;
    margin: 30rpx;
    padding: 10rpx;
    border-radius: 10rpx;
    color: #fff;
}
.zhuanchang{
    color: dimgray;
    font-size: small;
    line-height: 50rpx;
}
.shezhi{
    width: 70rpx;
    height: 70rpx;
    position: absolute;
    right: 10rpx;
    top: 10rpx;
}

/* pages/data/data.wxss */
.swiper-tab{
    width: 100%;
	border-bottom: 2rpx solid #777777;
	text-align: center;
    line-height: 80rpx;
    background-color: #fff;
}
.swiper-tab-list{  
    font-size: 30rpx;
    display: inline-block;
	width: 33.33%;
	color: #a7a6a6;
}
.on{ 
    color: #fff;
    background-color: #d7a4dd;
    border-bottom: 5rpx solid #fff;
}
 
.swiper-box{ 
    width: 100%; 
    height: 300rpx;
    display: block;
    overflow: hidden;
    
}
.swiper-box view{
	text-align: center;
}

.mid{
    width: 85%;
    height: 500rpx;
    background-color: #fff;
    margin: 0 auto;
    border-radius: 30rpx;
    color: #a1a0ee;
    padding: 20rpx;
    line-height: 90rpx;
    text-align: center;
    margin-top: 30rpx;
    box-shadow: 0 0 10rpx rgb(182, 182, 182);
}
.number{
    font-size: x-large;
    color: #f9dc74;
}
.shuju{
    width: 100%;
    height: 400rpx;
    background-color: #fff;
}
canvas{
    width: 100%;
    height: 100%;
}